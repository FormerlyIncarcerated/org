{
  "name": "formerly-incarcerated-empowerment",
  "version": "1.0.0",
  "private": false,
  "description": "Web3 empowerment platform for formerly incarcerated individuals - building second chances through blockchain technology",
  "keywords": [
    "web3",
    "empowerment",
    "formerly-incarcerated",
    "blockchain",
    "second-chance",
    "reentry",
    "social-impact"
  ],
  "author": "Formerly Incarcerated - No More Middleman",
  "homepage": "https://formerlyincarcerated.org",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "clean": "rimraf .next out dist",
    "clean:all": "rimraf .next out dist node_modules",
    "docs:dev": "cd docs-site && npm run start",
    "docs:build": "cd docs-site && npm run build",
    "docs:serve": "cd docs-site && npm run serve",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:reload": "pm2 reload ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:monit": "pm2 monit",
    "deploy:production": "pm2 deploy ecosystem.config.js production",
    "deploy:staging": "pm2 deploy ecosystem.config.js staging",
    "prepare": "husky install || true",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:a11y": "jest --testPathPattern=accessibility",
    "workflow:publish": "gh workflow run dual-repository-publish.yml",
    "workflow:publish-org": "gh workflow run dual-repository-publish.yml -f publish_to_org=true",
    "workflow:release": "gh workflow run dual-repository-publish.yml -f create_release=true",
    "sync:org": "node scripts/sync-to-org.js"
  },
